pipeline {
    agent any

    stages {
        stage('Build1') {
            steps {
                echo 'Building the code2'
            }
        }
        stage('Deploy1') {
            steps {
                echo 'Deploying the code'
            }
        }
        stage('Test1') {
            steps {
                echo 'Testing the code'
            }
        }
        stage('Release1') {
            steps {
                echo 'Release'
            }
        }
       
    }
}
